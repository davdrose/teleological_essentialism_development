        "instruction-survey-multiple": {
            "kind": "exp-lookit-survey",
            "formSchema": {
                "schema": {
                    "type": "object",
                    "properties": {
                        "parentresponse": {
                            "type": "string",
                            "title": "How should your child provide their answers?",
                            "enum": [
                                "Say their answers out loud.",
                                "Click the screen.",
                                "Smile.",
                                "Clap their hands."
                            ]
                        },
                        "qfeedback": {
                            "type": "text",
                            "title": "We can't see your child's answer if they point to things. It's important that they say their answers out loud."
                        },
                        "parentencourage": {
                            "type": "string",
                            "title": "What should you do if you need to repeat the questions to your child?",
                            "enum": [
                                "Quit.",
                                "Rephrase the question using your own words.",
                                "Don't rephrase the questions. Read the questions exactly as they appear in the 'For Parents' box.",
                                "Give them a hint."
                            ]
                        },
                        "encfeedback": {
                            "type": "text",
                            "title": "Since we are interested in how subtle changes in language affect categorization, it is VERY IMPORTANT that the questions NOT be rephrased."
                        },
                        "parentstopearly": {
                            "type": "string",
                            "title": "During the game, where are the instructions found?",
                            "enum": [
                                "In the 'For Parents' box at the bottom of the screen.",
                                "There are no instructions."
                            ]
                        },
                        "earlyfeedback": {
                            "type": "text",
                            "title": "It is important that you follow the instructions. These are found throughout the game in the 'For Parents' box at the bottom of the screen."
                        }
                    },
                    "dependencies": {
                        "qfeedback": [
                            "parentresponse"
                        ],
                        "encfeedback": [
                            "parentencourage"
                        ],
                        "earlyfeedback": [
                            "parentstopearly"
                        ]
                    }
                },
                "options": {
                    "fields": {
                        "parentresponse": {
                            "type": "radio",
                            "message": "Please select an answer",
                            "validator": "required-field",
                            "sort": false,
                            "removeDefaultNone": true,
                            "order": 1
                        },
                        "qfeedback": {
                            "type": "text",
                            "validator": "required-field",
                            "placeholder": "Try selecting a different answer above!",
                            "disabled": true,
                            "dependencies": {
                                "parentresponse": [
                                    "Click the screen.",
                                    "Smile.",
                                    "Clap their hands."
                                ]
                            },
                            "order": 2
                        },
                        "parentencourage": {
                            "type": "radio",
                            "message": "Please select an answer",
                            "validator": "required-field",
                            "sort": false,
                            "removeDefaultNone": true,
                            "order": 3
                        },
                        "encfeedback": {
                            "type": "text",
                            "validator": "required-field",
                            "placeholder": "Try selecting a different answer above!",
                            "disabled": true,
                            "dependencies": {
                                "parentencourage": [
                                    "Quit.",
                                    "Rephrase the question using your own words.",
                                    "Give them a hint."
                                ]
                            },
                            "order": 4
                        },
                        "parentstopearly": {
                            "type": "radio",
                            "message": "Please select an answer",
                            "validator": "required-field",
                            "sort": false,
                            "removeDefaultNone": true,
                            "order": 5
                        },
                        "earlyfeedback": {
                            "type": "text",
                            "validator": "required-field",
                            "placeholder": "Try selecting a different answer above!",
                            "disabled": true,
                            "dependencies": {
                                "parentstopearly": [
                                    "There are no instructions."
                                ]
                            },
                            "order": 6
                        }
                    }
                }
            }
        },
